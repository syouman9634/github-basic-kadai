	{
		String givenName = "花子";
		System.out.println("名前は加藤"+givenName+"です");
	}
	
	public void eachIntroduce()
	{
		String each = "趣味は読書です";
	}
	
	public void execIntroduce()
	{
		System.out.println("住所は"+address+"です");
		System.out.println("趣味は読書です");
	}

